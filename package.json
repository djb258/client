{
  "name": "client-subhive-integrated",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "description": "Integrated Client SubHive with IMO Creator and Barton Outreach Core",
  "main": "index.js",
  "scripts": {
    "migrate": "tsx scripts/run_migrations_via_mcp.ts",
    "export:run": "tsx scripts/run_vendor_export.ts",
    "registry:validate": "tsx scripts/validate_registry.ts",
    "promote": "tsx scripts/promote_to_neon.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:verbose": "jest --verbose",
    "dev": "vite",
    "build": "vite build",
    "build:dev": "vite build --mode development",
    "lint": "eslint .",
    "preview": "vite preview",
    "dev-ui": "cd apps/ui && npm run dev",
    "build-ui": "cd apps/ui && npm run build",
    "factory:dev": "node factory/ui/dev.js",
    "factory:build": "node factory/ui/build.js",
    "factory:init": "bash factory/ui/init.sh",
    "recall": "bash mechanic/recall/recall.sh",
    "env:check": "node scripts/env-check.mjs",
    "compliance:check": "python tools/repo_compliance_check.py",
    "compliance:fix": "python tools/repo_compliance_fixer.py",
    "audit": "python simple_garage_bay.py",
    "orchestrate": "python tools/repo_mcp_orchestrator.py",
    "vercel:dev": "vercel dev",
    "vercel:build": "mkdir -p public && cp index.html public/ && cp -r docs public/",
    "imo:list": "python tools/imo_unified_registry.py list-tools",
    "imo:list-category": "python tools/imo_unified_registry.py list-tools --category",
    "imo:list-integrations": "python tools/imo_unified_registry.py list-integrations",
    "imo:execute": "python tools/imo_unified_registry.py execute --tool",
    "imo:workflow": "python tools/imo_unified_registry.py workflow --repo-path . --workflow-type complete",
    "imo:workflow-quick": "python tools/imo_unified_registry.py workflow --repo-path . --workflow-type quick_check",
    "imo:workflow-fix": "python tools/imo_unified_registry.py workflow --repo-path . --workflow-type fix_only",
    "composio:setup": "python scripts/setup_composio_integrations.py",
    "composio:login": "composio login",
    "composio:connect-github": "composio add github",
    "composio:connect-slack": "composio add slack",
    "composio:connect-notion": "composio add notion",
    "composio:connect-gmail": "composio add gmail",
    "composio:connect-discord": "composio add discord",
    "composio:list-apps": "composio apps list",
    "composio:status": "python tools/composio_app_connector.py status",
    "composio:test": "python tools/composio_app_connector.py setup-defaults",
    "mcp:start-garage": "python garage-mcp/main.py",
    "mcp:test": "python tools/demo_workflow.py",
    "errors:check": "tsx scripts/check-errors.ts",
    "errors:unresolved": "tsx scripts/check-errors.ts unresolved",
    "errors:critical": "tsx scripts/check-errors.ts critical",
    "errors:summary": "tsx scripts/check-errors.ts summary",
    "errors:recent": "tsx scripts/check-errors.ts recent",
    "agent:validate-env": "tsx scripts/setup_agent_env.ts"
  },
